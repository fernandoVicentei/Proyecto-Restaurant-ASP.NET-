<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/pluggin/sweetalert2/sweetalert2.min.css">

    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet">

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/css/icomoon.css">
    <!-- Themify Icons-->
    <link rel="stylesheet" href="~/Content/css/themify-icons.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">

    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/css/magnific-popup.css">

    <!-- Bootstrap DateTimePicker -->
    <link rel="stylesheet" href="~/Content/css/bootstrap-datetimepicker.min.css">

    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/css/owl.theme.default.min.css">

    <!-- Theme style  -->
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link href="~/Content/Site.css" rel="stylesheet" />

    <!-- Modernizr JS -->
    <script src="~/Scripts/js/modernizr-2.6.2.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body style="padding-bottom:0px;padding-top:0px;">
    <div id="page">
        <!-- <div class="page-inner"> -->
        <nav class="gtco-nav bg-dark" role="navigation"style="background-color:black;position:relative;">
            <div class="gtco-container">
                <div class="row">
                    <div class="col-sm-4 col-xs-12">                        
                        <div id="gtco-logo"> <a href="../Productos/Catalogo"><img src="~/images/logo-min.jpeg" width="70px" height="70px" /></a><b  style="color:whitesmoke" class="cursive-font">FerAnVi - Foot <em>!</em></b></div>
                    </div>
                    <div class="col-xs-8 text-right menu-1">
                        <ul>
                            <li><a href="../Productos/Catalogo">Catalogo</a></li>
                            <li class="has-dropdown">
                                <a href="../Privado/Index">PANEL</a>
                            </li>
                            <li><a href="../About/Index">Quienes Somos?</a></li>
                            @{
                                if (Session["User"] == null)
                                {
                                    <li class="btn-cta"><a href="../Acceso/Login"><span>Iniciar Sesion</span></a></li>
                                }
                                else
                                {
                                    <li class="btn-cta"><a href="../Acceso/Login"><span>Cerrar Sesion</span></a></li>
                                }
                            }

                        </ul>
                    </div>
                </div>
            </div>
        </nav>      

        @RenderBody()

        <footer id="gtco-footer" role="contentinfo" style="background-image: url(images/img_bg_1.jpg)" data-stellar-background-ratio="0.5">
            <div class="overlay"></div>
            <div class="gtco-container">
                <div class="row row-pb-md">
                    <div class="col-md-12 text-center">
                        <div class="gtco-widget">
                            <h3>Get In Touch</h3>
                            <ul class="gtco-quick-contact">
                                <li><a href="#"><i class="icon-phone"></i> +1 234 567 890</a></li>
                                <li><a href="#"><i class="icon-mail2"></i> info@GetTemplates.co</a></li>
                                <li><a href="#"><i class="icon-chat"></i> Live Chat</a></li>
                            </ul>
                        </div>
                        <div class="gtco-widget">
                            <h3>Get Social</h3>
                            <ul class="gtco-social-icons">
                                <li><a href="#"><i class="icon-twitter"></i></a></li>                                
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-12 text-center copyright">
                        <p>
                            <small class="block">&copy; 2016 Free HTML5. All Rights Reserved.</small>
                            <small class="block">Designed by <a href="http://gettemplates.co/" target="_blank">GetTemplates.co</a> Demo Images: <a href="http://unsplash.com/" target="_blank">Unsplash</a></small>
                        </p>
                    </div>

                </div>
            </div>
        </footer>
        <!-- </div> -->
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/popper/popper.min.js"></script>
    <script src="~/Scripts/pluggin/sweetalert2/sweetalert2.all.min.js"></script>
    <script src="~/Scripts/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/js/magnific-popup-options.js"></script>
    <script src="~/Scripts/js/moment.min.js"></script>
    <script src="~/Scripts/js/bootstrap-datetimepicker.min.js"></script>
    <!-- jQuery -->
    <script src="~/Scripts/js/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/Scripts/js/jquery.easing.1.3.js"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <!-- Waypoints -->
    <script src="~/Scripts/js/jquery.waypoints.min.js"></script>
    <!-- Carousel -->
    <script src="~/Scripts/js/owl.carousel.min.js"></script>
    <!-- countTo -->
    <script src="~/Scripts/js/jquery.countTo.js"></script>
    <!-- Stellar Parallax -->
    <script src="~/Scripts/js/jquery.stellar.min.js"></script>
    <!-- Magnific Popup -->
    <script src="~/Scripts/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/js/magnific-popup-options.js"></script>
    <script src="~/Scripts/js/moment.min.js"></script>
    <script src="~/Scripts/js/bootstrap-datetimepicker.min.js"></script>
    <!-- Main -->
    <script src="~/Scripts/js/main.js"></script>
</body>
</html>
